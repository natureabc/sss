<script>
    var fid='${forumId}';
    $(function(){
        showDetail(fid);
        getSatuationList();
        showToolTitle();
        showKeywordList();
        showlabelList();

    });

    function showKeywordList(){
        $.ajax({
            type:"post",
            url:"/forum/getKeyWordList",
            success:function(data){
                if(data&&data.length>0){
                    var str="";
                    for(var i=0;i<data.length;i++){
                        str+=" <li><a href='#' onclick='toPageKeyword("+data[i].id+")'>"+data[i].keyword+"</a></li>";
                    }
                    $("#keywordList").html(str);
                }
            }
        });
    }

    function showlabelList(){
        $.ajax({
            type:"post",
            url:"/forum/getLabelList",
            success:function(data){
                if(data&&data.length>0){
                    var str="";
                    for(var i=0;i<data.length;i++){
                        str+="  <a href='#' onclick='showList("+data[i].id+")'>"+data[i].labelName+"</a>";
                    }
                    $("#labelList").html(str);
                }
            }
        });
    }

    function showDetail(fid){

        $.ajax({
            type:"post",
            url:"/forum/getForumDetailById",
            data:{"forumId":fid},
            success:function(data){
                var forumPic="<img  src='"+data.mainPic+"'/>";
                $("#forumPic").html(forumPic);
                $("#forumTitle").html(data.title);
                $("#forumTitle").attr("title",data.title)
                var forumKeyWord="发布于 <a href='#' >"+data.keyWord+"</a>&nbsp;" +data.createTimeStr+"&nbsp;["+data.browseNum+" ] 次浏览 ";
                $("#forumKeyWord").html(forumKeyWord);
                var keywordNav=" > <a href='#'>"+data.keyWord+" </a>  >"
                $("#keywordNav").html(keywordNav);
                $("#titleNav").html(data.title);
                var forumLabel="标签: &nbsp;";
                if(data&&data.labelList.length>0){
                    for(var i=0;i<data.labelList.length;i++){
                        forumLabel+="<a href='#'>"+data.labelList[i].labelName+"</a>&nbsp;";
                    }
                }
                $("#forumLabel").html(forumLabel);

                var oIframe = document.getElementById("iframe1");
                oIframe.contentWindow.document.body.innerHTML= data.content;
                var iframeHeight=oIframe.contentWindow.document.body.offsetHeight;
                document.getElementById("forumContent").style.height= iframeHeight+"px";

                if(data.preId&&data.nextId){
                    var str="<span title='上一篇'>上一篇:</span><a href='#' onclick='changeDetail("+data.preId+")'>"+data.preTitle+"</a><br>";
                    str+="<span title='下一篇'>下一篇:</span><a href='#' onclick='changeDetail("+data.nextId+")'>"+data.nextTitle+"</a>";
                    $("#neighborLog").html(str);
                }
                if(data.preId&&!data.nextId){
                    var preStr="<span title='上一篇'>上一篇:</span><a href='#' onclick='changeDetail("+data.preId+")'>"+data.preTitle+"</a>";
                    $("#neighborLog").html(preStr);
                }
                if(!data.preId&&data.nextId){
                    var nextStr="<span title='下一篇'>下一篇:</span><a href='#' onclick='changeDetail("+data.nextId+")'>"+data.nextTitle+"</a>";
                    $("#neighborLog").html(nextStr);
                }

            }

        });

    }

    function changeDetail(id){
        showDetail(id);
    }


    function showToolTitle(){
        $.ajax({
            type:"post",
            url:"/forum/getToolTitle",
            success:function(data){
                if(data){
                    var str="<li class='current-cat'><a href='/forum/index'>首页</a></li>";
                    // console.log(data)
                    for(var i=0;i<data.length;i++){
                        str+="<li class='common'><a href='/forum/index'>"+data[i].labelName+"</a></li>"
                    }
                    $("#nv").html(str);
                }
            }
        })

    }

    function getSatuationList(){

        $.ajax({
            type:"post",
            url:"/forum/getSatuationList",
            success:function(data){
                if(data&&data.length>0){
                    var str="";
                    for(var i=0;i<data.length;i++){
                        str+=" <li><span style='display:block; float:left; width:197px; overflow:hidden;'>"+data[i].message+"</span></li>";
                    }
                    $("#newSatuation").html(str);
                }
            }

        });

    }

</script>
