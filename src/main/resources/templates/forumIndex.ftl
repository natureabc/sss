    <script>
        $(function (){
            showList();
            getSatuationList();
            showKeywordList();
            showlabelList();
        })

        function showList(labelId,keywordId){
            $.ajax({
                type:"post",
                url:"/forum/getAllList",
                data:{"labelId":labelId,"keywordId":keywordId},
                success:function(data){
                    if(data&&data.length>0){
                        var str="";
                        for(var i=0;i<data.length;i++){
                            var labels="";
                            if(data[i].labelList&&data[i].labelList.length>0){
                                for(var l=0;l<data[i].labelList.length;l++){
                                    labels+="<a href='#' onclick='toPageByLabel("+data[i].labelList[l].labelId+")'>"+data[i].labelList[l].labelName+"&nbsp;"+"</a>"
                                }
                            }

                            str+="<div class='lists'>" +
                                "            <div class='lists_l'>" +
                                "                <a href='#' onclick='toForumDetail("+data[i].id+")'>" +
                                "                    <img src="+data[i].mainPic+""+
                                "                          height='49px' width='49px'/>" +
                                "                </a>" +
                                "            </div>" +
                                "            <div class='lists_m'>" +
                                "                <div class='box'>" +
                                "                    <div class='title'>" +
                                "                        <a href='#' rel='bookmark'" +
                                "                           title='"+data[i].title+"'  onclick='toForumDetail("+data[i].id+")'>" +
                                "                                   <font color=#ff0000 style=font-weight:550>"+data[i].title+"</font></a>" +
                                "                    </div>" +
                                "                    <div class='read_total'><a href='#' onclick='toPageKeyword("+data[i].keywordId+")'>" +
                                "                        "+data[i].keyWord+"</a>"+
                                "                        标签: "+labels+
                                "<a href='#'>&nbsp浏览："+data[i].browseNum+"</a>"+
                                "                    </div>" +
                                "                </div>" +
                                "                <div class='lists_r'>" +
                                "                    <div class='info'>" +
                                "                    </div>" +
                                "                    <span id='comm'>" +
                                "<span id='views'>" +
                                "                </div>" +
                                "            </div>" +
                                "            <div style='clear:both'></div>" +
                                "        </div>"
                        }

                        $("#allForumList").html(str);

                    }
                },
                error:function(data){
                }
            })
        }



        function toPageByLabel(labelId){
            showList(labelId);
        }

        function toForumDetail(id){

            var str="";
            str+="<input type='hidden' name='id' value='"+id+"'/>";
            $("#tempForm").html(str);
            $("#tempForm").attr("action","/forum/toDetail");
            $("#tempForm").attr("method","post");
            $("#tempForm").submit();
            //window.location.href="/forum/toDetail?id="+id;

        }

        function getSatuationList(){

            $.ajax({
                type:"post",
                url:"/forum/getSatuationList",
                success:function(data){
                    if(data&&data.length>0){
                        var str="";
                        for(var i=0;i<data.length;i++){
                            str+=" <li><span style='display:block; float:left; width:197px; overflow:hidden;'>"+data[i].message+"</span></li>";
                        }
                        $("#newSatuation").html(str);
                    }
                }

            });
        }

        function showKeywordList(){
            $.ajax({
                type:"post",
                url:"/forum/getKeyWordList",
                success:function(data){
                    if(data&&data.length>0){
                        var str="";
                        for(var i=0;i<data.length;i++){
                            str+=" <li><a href='#' onclick='toPageKeyword("+data[i].id+")'>"+data[i].keyword+"</a></li>";
                        }
                        $("#keywordList").html(str);
                    }
                }
            });
        }


        function toPageKeyword(id){

            showList(null,id);
        }

        function showlabelList(){
            $.ajax({
                type:"post",
                url:"/forum/getLabelList",
                success:function(data){
                    if(data&&data.length>0){
                        var str="";
                        for(var i=0;i<data.length;i++){
                            str+="  <a href='#' onclick='showList("+data[i].id+")'>"+data[i].labelName+"</a>";
                        }
                        $("#labelList").html(str);
                    }
                }
            });
        }

    </script>

